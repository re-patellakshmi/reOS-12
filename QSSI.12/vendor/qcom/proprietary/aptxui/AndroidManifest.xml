<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qualcomm.qtil.aptxui"
    android:versionCode="1"
    android:versionName="4.0.6">

    <uses-sdk android:minSdkVersion="31"
            android:targetSdkVersion="31"
            android:maxSdkVersion="31" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="com.qualcomm.qtil.aptxals.PERMISSION"/>

    <permission
        android:name="com.qualcomm.qtil.aptxui.PERMISSION"
        android:label="aptxui_permission"
        android:protectionLevel="signature">
    </permission>

    <application
        android:name=".aptxuiApplication"
        android:allowBackup="false"
        android:theme="@style/Theme.AppCompat.DayNight"
        android:label="@string/app_name"
        android:directBootAware="true">
        <receiver
            android:name=".aptxuiBootReceiver"
            android:label="aptxuiBootReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.LOCKED_BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
       <service
            android:name=".aptxuiService"
            android:permission="com.qualcomm.qtil.aptxui.PERMISSION"
            android:enabled="true"
            android:exported="true">
            <intent-filter >
                <action android:name="com.qualcomm.qtil.aptxals.aptxuiService"/>
            </intent-filter>
       </service>
       <service
            android:name=".aptxuiNotify"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:enabled="true"
            android:exported="false">
       </service>
        <receiver
            android:name=".aptxuiNotifyReceiver"
            android:label="aptxuiNotifyReceiver"
            android:enabled="true"
            android:exported="false">
        </receiver>
    </application>
</manifest>
